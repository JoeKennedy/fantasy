User
    firstName Text Maybe
    lastName Text Maybe
    isAdmin Bool default=false
    deriving Show Typeable
Email
    email Text
    userId UserId
    verkey Text Maybe
    UniqueEmail email
Ident
    ident Text
    userId UserId
    UniqueIdent ident

Species
    name Text
    description Text
    UniqueSpeciesName name
    UniqueSpeciesDescription description
House
    name Text
    words Text
    description Text
    UniqueHouseName name

Character
    name Text
    bio Text
    speciesId SpeciesId
    houseId HouseId Maybe
    status CharacterStatus
    createdBy UserId
    createdAt UTCTime
    updatedBy UserId
    updatedAt UTCTime
    UniqueCharacterName name
    UniqueCharacterBio bio

Blurb
    characterId CharacterId
    content Textarea
    createdBy UserId
    createdAt UTCTime
    updatedBy UserId
    updatedAt UTCTime
    UniqueBlurbContent content

Series
    number Int
    UniqueSeriesNumber number
    deriving Eq Ord
Episode
    name Text
    number Int
    overallNumber Int
    airTime UTCTime
    seriesId SeriesId
    UniqueEpisodeOverallNumber overallNumber
    UniqueEpisodeNumberSeries number seriesId
    UniqueEpisodeAirTime airTime
    UniqueEpisodeName name
Event
    characterId CharacterId
    action Action
    receivingCharacterId CharacterId Maybe
    episodeId EpisodeId
    note Text Maybe
    timeInEpisode Int
    UniqueEvent characterId action episodeId timeInEpisode

League
    name Text
    isActive Bool default=true
    isPrivate Bool default=false
    scoringType ScoringType
    teamsCount Int
    isSetupComplete Bool default=false
    lastCompletedStep Int default=1
    isDraftComplete Bool default=false
    createdBy UserId
    createdAt UTCTime
    updatedBy UserId
    updatedAt UTCTime
    draftCompletedAt UTCTime Maybe
    UniqueLeagueName name

GeneralSettings
    leagueId LeagueId
    numberOfStarters Int
    rosterSize Int
    regularSeasonLength Int
    playoffLength Int
    numberOfTeamsInPlayoffs Int
    tradeDeadlineWeek Int
    waiverPeriodInDays Int
    createdBy UserId
    createdAt UTCTime
    updatedBy UserId
    updatedAt UTCTime
    UniqueGeneralSettingsLeagueId leagueId

ScoringSettings
    leagueId LeagueId
    action Action
    isUsed Bool
    points Int
    weight Int
    pointsReceiving Int
    weightReceiving Int
    createdBy UserId
    createdAt UTCTime
    updatedBy UserId
    updatedAt UTCTime
    UniqueScoringSettingsLeagueIdAction leagueId action

DraftSettings
    leagueId LeagueId
    draftType DraftType
    draftOrder DraftOrder
    draftOrderType DraftOrderType
    date Day
    time TimeOfDay
    location Text
    allowDraftPickTrading Bool
    secondsPerPick Int
    note Textarea
    createdBy UserId
    createdAt UTCTime
    updatedBy UserId
    updatedAt UTCTime
    UniqueDraftSettingsLeagueId leagueId

Team
    leagueId LeagueId
    name Text
    abbreviation Text
    ownerId UserId Maybe
    ownerName Text
    ownerEmail Text
    isConfirmed Bool default=false
    playersCount Int default=0
    startersCount Int default=0
    draftOrder Int default=0
    waiverOrder Int default=0
    verificationKey Text
    createdBy UserId
    createdAt UTCTime
    updatedBy UserId
    updatedAt UTCTime
    confirmedBy UserId Maybe
    confirmedAt UTCTime Maybe
    UniqueTeamNameLeagueId name leagueId
    UniqueTeamAbbreviationLeagueId abbreviation leagueId
    UniqueTeamOwnerEmailLeagueId ownerEmail leagueId
    deriving Eq Ord

Player
    leagueId LeagueId
    characterId CharacterId
    teamId TeamId Maybe
    isStarter Bool default=false
    createdBy UserId
    createdAt UTCTime
    updatedBy UserId
    updatedAt UTCTime
    UniquePlayerCharacterLeague characterId leagueId

Transaction
    leagueId LeagueId
    type TransactionType
    status TransactionStatus
    failureReason Text Maybe
    teamId TeamId
    otherTeamId TeamId Maybe
    createdBy UserId
    createdAt UTCTime
    updatedBy UserId
    updatedAt UTCTime
    processableAt UTCTime default=now()
    completedAt UTCTime Maybe
    deriving Eq

TransactionPlayer
    leagueId LeagueId
    transactionId TransactionId
    playerId PlayerId
    oldTeamId TeamId Maybe
    newTeamId TeamId Maybe
    createdBy UserId
    createdAt UTCTime
    updatedBy UserId
    updatedAt UTCTime
    UniqueTransactionPlayerTransactionIdPlayerId transactionId playerId


